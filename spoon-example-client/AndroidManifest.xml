<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
  package="com.willowtreeapps.example.spoon.app" android:versionCode="1" android:versionName="1.0-SNAPSHOT">

    <uses-sdk android:minSdkVersion="16" android:targetSdkVersion="16"/>

    <!-- Used by instrumentation tests only. Do not include in production apps unless necessary! -->
    <uses-permission android:name="android.permission.DISABLE_KEYGUARD"/>
    <uses-permission android:name="android.permission.WAKE_LOCK"/>

  <application
	  android:icon="@drawable/ic_launcher"
      android:label="@string/app_name"
      android:allowBackup="true"
      android:theme="@android:style/Theme.Holo.Light.DarkActionBar">

      <activity android:name=".MainActivity">
          <intent-filter>
              <action android:name="android.intent.action.MAIN" />
              <category android:name="android.intent.category.LAUNCHER" />
          </intent-filter>
      </activity>
      <activity android:name=".OtherActivity" android:label="Other Activity"/>

  </application>

</manifest>

